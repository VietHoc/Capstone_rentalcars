 <tbody>
 	<% requesting.each do |e| %>
      <tr>
        <td><%= e.user_id %></td>
        <td><%= e.pickup %></td>
        <td><%= e.dropoff %> </td>
        <td><%= e.status%> </td>
        <td><%= e.verified%></td>
        <td><%= e.car_id%> </td>
        <%= render 'shared/handing', object: e %>
      </tr>
    <%end%>
  </tbody>
